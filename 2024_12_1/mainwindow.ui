          <property name="styleSheet">
           <string notr="true">/* QGroupBox 的整体样式 */
QGroupBox {
    background-color: #F5F5F5;
    border: 2px solid #B0BEC5;
    border-radius: 8px;
    margin-top: 20px; /* 控制标题距离上方的间距 */
    padding: 0px; /* 内部留出空间 */
}
/* 标题的样式，设置在左上角 */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left; /* 将标题对齐到左上角 */
    padding: 0 8px; /* 调整标题的内边距 */
    color: #37474F;
    font-size: 16px;
    font-weight: bold;
    background-color: transparent; /* 可选，设置为透明以便更好地融入背景 */
}

/* 鼠标悬停时的边框颜色 */
QGroupBox:hover {
    border: 2px solid #4CAF50;
}

/* QGroupBox 内部 QTableWidget 的样式 */
QGroupBox QTableWidget {
    border: 1px solid #B0BEC5;
    background-color: #FAFAFA;
    gridline-color: #CFD8DC;
    font-size: 14px;
    color: #37474F;
}

/* QTableWidget 的交替行颜色 */
QGroupBox QTableWidget::item {
    background-color: #FFFFFF;
}

QGroupBox QTableWidget::item:alternate {
    background-color: #F5F5F5;
}

/* 鼠标悬停效果 */
QGroupBox QTableWidget::item:hover {
    background-color: #E8F5E9;
}

/* 选中行的样式 */
QGroupBox QTableWidget::item:selected {
    background-color: #4CAF50;
    color: white;
}

/* 表头样式 */
QGroupBox QHeaderView::section {
    background-color: #ECEFF1;
    padding: 8px;
    font-weight: bold;
    color: #607D8B;
    border: none;
    border-bottom: 1px solid #B0BEC5;
}

/* 设置垂直滚动条的样式 */
QGroupBox QScrollBar:vertical {
    border: none;
    background: #E0E0E0;
    width: 8px;
    margin: 22px 0 22px 0;
}

QGroupBox QScrollBar::handle:vertical {
    background: #4CAF50;
    min-height: 20px;
    border-radius: 4px;
}

/* 隐藏滚动条的上下线 */
QGroupBox QScrollBar::add-line:vertical, QGroupBox QScrollBar::sub-line:vertical {
    height: 0px;
    width: 0px;
    subcontrol-origin: margin;
}

QGroupBox QScrollBar::add-page:vertical, QGroupBox QScrollBar::sub-page:vertical {
    background: none;
}
